<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="meymard_core.form_notetype.class">Meymard\CoreBundle\Form\NoteType</parameter>
    </parameters>

    <services>
        <service id="meymard_core.form_notetype" class="%meymard_core.form_notetype.class%">
            <tag name="form.type" alias="meymard_note" />
        </service>
    </services>
</container>
